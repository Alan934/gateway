{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "npm run build",
  "routes": [
    {
    "src": "/api/users/(.*)",
    "dest": "/api/users",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    }
  ]
}
  